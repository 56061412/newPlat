	<package name="snsKnowledge" extends="struts-default" namespace="/snsKnowledge">
		<action name="addKnowledgeInfo" class="com.auto.sns.knowledge.action.SnsKnowledgeAction"
			method="addKnowledgeInfo">
		</action>
		<action name="listKnowledgeInfoGroup" class="com.auto.sns.knowledge.action.SnsKnowledgeAction"
			method="listKnowledgeInfoGroup">
			<result name="success">/listToJson.jsp</result>
		</action>
		<action name="listKnowledgeInfoInfo" class="com.auto.sns.knowledge.action.SnsKnowledgeAction"
			method="listKnowledgeInfoInfo">
			<result name="success">/listToJson.jsp</result>
		</action>
	</package>
	
